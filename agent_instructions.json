{
  "wallet": {
    "address_env": "WALLET_ADDRESS",
    "private_key_env": "WALLET_PRIVATE_KEY"
  },
  "predictor": {
    "api_endpoint_env": "PREDICTOOR_API",
    "feeds": [
      "BTC/USDT",
      "ETH/USDT",
      "SOL/USDT",
      "MATIC/USDT"
    ],
    "prediction_interval_seconds": 1800,
    "claim_interval_seconds": 14400,
    "higher_confidence": true
  },
  "gelato": {
    "relayer_env": "GELATO_RELAYER",
    "auto_select_best": true
  },
  "telegram": {
    "bot_token_env": "TELEGRAM_BOT_TOKEN",
    "chat_id_env": "TELEGRAM_CHAT_ID"
  },
  "logging": {
    "performance_file": "performance_log.json",
    "claim_file": "claim_log.json",
    "error_file": "error_log.json"
  },
  "agentic_ai": {
    "enabled": true,
    "auto_restart_on_error": true
  }
}
